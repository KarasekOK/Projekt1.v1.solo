                    print("Moje puvodni vydrz: ""(", vyd, ")")
                    vyd = vyd - dmg / 100 * procento
                    print(prehled)
                    print("*** Moje vydrz: ""(", vyd, ")")
                    print(prehled)
                    print("Muj dmg: ""(", dmg / 100 * 40, ")")
                    print(prehled)
                    print("Enemy puvodni vydrz: ""(", nepritel, ")")
                    nepritel = nepritel - pos / 100 * 40
                    print(prehled)
                    print("*** Enemy vydrz: ""(", nepritel, ")")
                    print(prehled)
                    print("Enemy dmg: ""(", dmg / 100 * procento, ")")
                    print(prehled)
                    kolo = kolo + 1
                    if nepritel <= 0:
                        print("GG")
                        print(nepritel)
                        print(
                            "Máš navýber novou akci jménem Omráčení omráčí nepřitele na 2 kola po použití se dá znovu použít až po 4 kolách.")
                        bod = bod + 1
                        kolo = 1
                        doba = 0
                        S = 0
                        break
                    if vyd <= 0:
                        print("Prohra")
                        print(vyd)
                        kolo = 1
                        break
                    if vyd and nepritel <= 0:
                        print("Remíza")
                        bod = 0
                        kolo = 1
                        break
