import random

def sila():
    sil = 40 + bod *15
    vyd = 100 + bod *10
    return sil, vyd

def vyda():
    sil = 20 + bod*10
    vyd = 200 + bod*15
    return sil, vyd

def enemy():
    ed = 10 + bod * 12
    med = 45 + bod * 12
    ehp = 50 + bod * 12
    mehp = 100 + bod *12
    return ed , med , ehp , mehp
kolo = 1
prehled = ("////////////////////////////")
bod = 0
doba = 0
